    LinkedSockets = 6
    Corrupted True
    SetBorderColor 150 0 0
    SetBackgroundColor 250 250 250
    SetTextColor 250 225 0
    SetFontSize 45
    PlayAlertSound 8 300
    MinimapIcon 0 White Star
    PlayEffect White
